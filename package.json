{
  "name": "opendsm-client",
  "version": "0.0.1",
  "description": "The client application for OpenDSM - The Open Digital Software Marketplace",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "package-win": "electron-packager . OpenDSM --platform=win32 --out ./builds --overwrite --icon=src\\assets\\images\\icons\\icon.ico",
    "package-linux": "electron-packager . OpenDSM --platform=linux --out ./builds --overwrite --icon=src\\assets\\images\\icons\\logo.png",
    "package-mac": "electron-packager . OpenDSM --platform=darwin,mas --out ./builds --overwrite --icon=src\\assets\\images\\icons\\logo.png"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DcmanProductions/OpenDSM-Client.git"
  },
  "keywords": [
    "DSM",
    "Open",
    "DSM",
    "OpenSource",
    "Digital",
    "Software",
    "Marketplace"
  ],
  "author": "Drew Chase",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DcmanProductions/OpenDSM-Client/issues"
  },
  "homepage": "https://github.com/DcmanProductions/OpenDSM-Client#readme",
  "devDependencies": {
    "electron": "^20.1.0"
  }
}
